{
  "name": "custom-electron-prompt",
  "version": "1.2.0",
  "description": "Custom prompt for Electron made easy with various templates",
  "homepage": "https://araxeus.github.io/custom-electron-prompt",
  "keywords": [
    "electron",
    "input",
    "select",
    "counter",
    "accelerator",
    "keybind",
    "prompt",
    "dialog"
  ],
  "author": {
    "name": "Araxeus",
    "email": "araxeus@users.noreply.github.com",
    "url": "https://github.com/araxeus"
  },
  "repository": "github:araxeus/custom-electron-prompt",
  "license": "MIT",
  "main": "lib/index",
  "scripts": {
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "npm run lint"
  },
  "xo": {
    "esnext": true,
    "env": [
      "node",
      "browser"
    ],
    "rules": {
      "unicorn/prefer-ternary": 0,
      "quotes": [
        "error",
        "double",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "peerDependencies": {
    "electron": "^10.0.0 || ^11.0.0 || ^12.0.0"
  },
  "devDependencies": {
    "xo": "^0.38.2"
  }
}
